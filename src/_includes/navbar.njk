<div id="navbar" class="navbar">
	<input type="checkbox" id="navbar-toggle" class="navbar-toggle">
	<label for="navbar-toggle" class="hamburger">
		Start
	</label>
  
	<div class="nav-links">
	{% for link in navbar_links %}
		<a href="#" class="app-launcher" data-title="{{ link.title }}" data-url="{{ link.url }}" data-id="{{ link.id }}">{{ link.title }}</a>
	{% endfor %}
	<button id="close-all-windows">Close All Windows</button>
	</div>
	
	<div id="date-time" class="date-time">
		<div id="date"></div> 
		<span>|</span>
		<div id="clock">8:10:45</div>
	</div>
	
	<div id="calendar-container" class="calendar-hidden"></div>
	
</div>

